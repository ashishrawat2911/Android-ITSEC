<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="in.edu.itsecgn.itsengineeringcollege">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.LoginActivity" />
        <activity android:name=".activities.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.EDActivity"
            android:parentActivityName=".activities.MainActivity" />
        <activity
            android:name=".activities.HODActivity"
            android:parentActivityName=".activities.MainActivity" />
        <activity
            android:name=".activities.MentorActivity"
            android:parentActivityName=".activities.MainActivity" />
        <activity
            android:name=".activities.ExploreITSActivity"
            android:parentActivityName=".activities.MainActivity" />
        <activity
            android:name=".activities.OrientationScheduleActivity"
            android:parentActivityName=".activities.MainActivity" />
        <activity
            android:name=".activities.StudentHandbook"
            android:parentActivityName=".activities.MainActivity" />
        <activity
            android:name=".activities.TimeTableActivity"
            android:parentActivityName=".activities.MainActivity" />
        <activity
            android:name=".activities.SyllabusActivity"
            android:parentActivityName=".activities.MainActivity" />
        <activity
            android:name=".activities.AnnualCalenderActivity"
            android:parentActivityName=".activities.MainActivity" />
        <activity
            android:name=".activities.BusRouteActivity"
            android:parentActivityName=".activities.MainActivity" />
        <activity
            android:name=".activities.SportsActivity"
            android:parentActivityName=".activities.MainActivity" />
        <activity
            android:name=".activities.HostelRulesActivity"
            android:parentActivityName=".activities.MainActivity" />
        <activity
            android:name=".activities.ExtraCurricularActivity"
            android:parentActivityName=".activities.MainActivity" />
        <activity
            android:name=".activities.NewsAndEventsActivity"
            android:parentActivityName=".activities.MainActivity" />
        <activity
            android:name=".activities.SocialMediaActivity"
            android:parentActivityName=".activities.MainActivity" />
        <activity
            android:name=".activities.NoticeAndAnnouncementsActivity"
            android:parentActivityName=".activities.MainActivity" />
        <activity
            android:name=".activities.MainActivity"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>